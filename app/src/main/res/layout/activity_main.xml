<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fafafa"
    android:orientation="vertical"
    tools:context=".MainActivity">
    <fragment
        android:id="@+id/fragment_search_page"
        android:name="com.moyu.browser_moyu.searchpage.activity.SearchPageFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
    />

    <fragment
        android:id="@+id/fragment_navigation_list"
        android:name="com.moyu.browser_moyu.navigationlist.activity.NavigationListFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="15"
        />


</LinearLayout>